define("common:widget/verificationCode/useAIVerificationCode",function(i,e,n){var c,o=i("common:widget/ui/base/base"),a=i("common:widget/ui/base/events"),s=o.extend({init:function(){o.getScript("https://wappass.baidu.com/static/machine/js/api/mkd.js",function(){if(window.PassMachine&&window.PassMachine.mkd){var i=window.PassMachine.mkd;c=new i({type:"slide",ak:"5c55ec1d6f983c7c410423a63d2071b6",maskModule:!0,verifySuccessFn:function(i){c.removeMask(),i.acjsonfr=c.acjsonfr,a.trigger("verificationParam",i)},closeFn:function(){a.trigger("verificationCloseFn",!1)}})}})},initVcode:function(i){c&&(c.acjsonfr=i,c.initVcode())}});n.exports=s});